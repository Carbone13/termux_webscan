#bismillah
clear

echo ""
echo -e "\033[31m   ██   ██ ████████ ████████ ███████ \033[0m"
echo -e "\033[31m   ██   ██    ██       ██    ██   ██ \033[0m"
echo -e "\033[31m   ███████    ██       ██    ███████ \033[0m"
echo -e "\033[31m   ██   ██    ██       ██    ██ \033[0m"
echo -e "\033[31m   ██   ██    ██       ██    ██ \033[0m"
echo -e " "
echo -e "\033[31m    ███ ███  ███████ ████████ ██   ██ ██████ ████ \033[0m"
echo -e "\033[31m   ██ ███ ██ ██         ██    ██   ██ ██  ██ ██ ██ \033[0"
echo -e "\033[31m   ██  █  ██ ███████    ██    ███████ ██  ██ ██  ██ \033[0m"
echo -e "\033[31m   ██     ██ ██         ██    ██   ██ ██  ██ ██ ██ \033[0m"
echo -e "\033[31m   ██     ██ ███████    ██    ██   ██ ██████ ████ \033[0m"
echo -e "\033[31m #######################################################\033[0m"
echo -e "\033[31m   Author : 7grok \033[0m"
echo -e "\033[31m   Tools  : http-methods \033[0m"
echo -e "\033[31m #######################################################\033[0m"
echo ""

echo -n "[*] Masukan Url : "
read anu
if [ ! -z $anu ]
then
echo "[*] Scanning... "
nmap -p80 -sV --script=http-methods $anu
else
echo "[!] Url Belum Di Masukan! "
fi
